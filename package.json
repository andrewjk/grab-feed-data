{
  "name": "grab-feed-data",
  "version": "1.0.1",
  "description": "Grabs feed and entry data out of an RSS, Atom or JSON Feed file",
  "keywords": [
    "rss",
    "atom",
    "jsonfeed",
    "feed"
  ],
  "author": "andrewjk",
  "license": "ISC",
  "homepage": "https://github.com/andrewjk/grab-feed-data",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "browser": "dist/index.mjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "uvu -r tsm tests",
    "check": "tsc -p tsconfig-check.json --noEmit",
    "release": "tsup && np"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "np": "^7.7.0",
    "tsm": "^2.3.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "grab-xml": "^1.0.2"
  }
}
